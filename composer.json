{
    "name": "minhyung/kexim",
    "description": "Korea Eximbank OpenAPI wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Minhyung Park",
            "email": "minhyung.park@sejongtf.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Minhyung\\Kexim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minhyung\\Kexim\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
